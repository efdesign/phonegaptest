<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap       = "http://phonegap.com/ns/1.0"
    xmlns:android   = "http://schemas.android.com/apk/res/android"
    id              = "it.enricofurlan.orderit"
    versionCode     = "10" 
    version         = "1.0.0" >

    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <gap:plugin name="com.jamiestarke.webviewdebug" version="1.0.8" />
    <!-- versionCode is optional and Android only -->

    <name>PhoneGap Example</name>

    <description>
      An example for phonegap build docs. 
    </description>

    <author href="https://build.phonegap.com" email="support@phonegap.com">
      Hardeep Shoker 
    </author>

    <feature name="debug-server" required="true">
    <param name="192.168.0.101" value="http://localhost:8080"/>
    <param name="key" value="weinre"/>
  </feature>



    <!--
      This allows your app to load assets from all *.phongap.com domains
    -->
    <access origin="*" subdomains="true" />
    <gap:platform name="android" />
    <preference name="fullscreen" value="true" />
    <preference name="orientation" value="portrait" />
    
    <icon src="logo-regular.png" gap:platform="android" gap:qualifier="ldpi" />
    <icon src="logo-mdpi.png" gap:platform="android" gap:qualifier="mdpi" />
    <icon src="logo-hdpi.png" gap:platform="android" gap:qualifier="hdpi" />
</widget>